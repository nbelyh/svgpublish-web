{
  "extends": "./../../node_modules/@microsoft/rush-stack-compiler-5.3/includes/tsconfig-web.json",
  "compilerOptions": {
    "target": "ES2020",
    "forceConsistentCasingInFileNames": true,
    "module": "es2020",
    "moduleResolution": "node",
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "inlineSources": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "rootDir": "src",
    "typeRoots": [
      "./../../node_modules/@types",
    ],
    "types": [
      "webpack-env",
    ],
    "lib": [
      "DOM",
      "ES2020"
    ],
    "baseUrl": "./src"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}
